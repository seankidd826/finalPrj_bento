



<div class="container">
 <div class="row">
  <table class="table table-striped">
    <hr>
      <tr>
        <th>
          <strong>姓名:</strong>
        </th>
        <td><%= @admin_user.name %></td>
      </tr>

      <tr>
        <th>
          <strong>電話:</strong>
        </th>
        <td><%= @admin_user.phone %></td>
      </tr>

      <tr>
        <th>
          <strong>地址:</strong>
        </th>
        <td><%= @admin_user.address %></td>
      </tr>

      <tr>
        <th>
          <strong>Email:</strong>
        </th>
